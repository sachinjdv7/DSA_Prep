
let x = 10;

let y = x

x = 20

console.log(x) //20
console.log(y) // 10

// call by reference


let a = { x: 5, y: 10 }

let b = JSON.parse(JSON.stringify(a));

a.x = 15

console.log(a)
console.log(b);


// function 



const number = 10

let obj = { val: 10 }

function increment(obj) {
    // here we copied value 
    obj.val++
}

increment(obj)
console.log(obj.val)